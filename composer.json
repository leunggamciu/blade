{
    "name": "leunggamciu/blade",
    "description": "A view package extracted from laravel that does not depend on laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "leunggamciu",
            "email": "gamciuleung@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/events": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "illuminate/support": "5.6.*",
        "symfony/debug": "~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Blade\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blade\\Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
